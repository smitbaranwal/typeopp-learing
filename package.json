{
  "name": "typeopp-learning",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.config.json",
    "node:start": "node server.js",
    "start:node": "ng build && node server.js",
    "build": "ng build",
    "test": "npm run lint && ng test --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --browsers ChromeHeadless --reporters spec",
    "build:prod": "ng build --target production --build-optimizer --vendor-chunk --stats-json",
    "build:travisci": "ng build --target production --build-optimizer --vendor-chunk --deploy-url /epricer/ --base-href /epricer",
    "clean": "rm -rf ./dist/",
    "server": "cd dist && http-server",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "node:prod": "npm run clean && npm run build:prod && npm run start:node",
    "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --browser ChromeTravisCi --single-run --reporters spec --environment prod && npm run build:travisci",
    "release": "standard-version && git push --follow-tags origin master",
    "prettier": "prettier --single-quote --parser typescript --write \"./src/**/*.ts\" && prettier --single-quote --parser scss --write \"./src/**/*.scss\"",
    "prettier:ci": "prettier --single-quote --parser typescript --list-different \"./src/**/*.ts\" && prettier --single-quote --parser scss --list-different \"./src/**/*.scss\"",
    "analyze": "npm run clean && npm run build:prod && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "ag-grid": "^17.0.0",
    "ag-grid-angular": "^17.0.0",
    "ag-grid-enterprise": "^17.0.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "ngx-bootstrap": "^2.0.5",
    "njwt": "^0.4.0",
    "rxjs": "^5.5.6",
    "superagent": "^3.8.2",
    "uuid": "^3.2.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.106",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "enhanced-resolve": "^4.0.0",
    "install": "^0.11.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "npm": "^5.8.0",
    "prettier": "^1.11.1",
    "protractor": "~5.1.2",
    "standard-version": "^4.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
